{
  "name": "halogen-frontend",
  "version": "1.0.0",
  "description": "Halogen frontend for Haskell backend",
  "main": "index.js",
  "scripts": {
    "build": "spago build",
    "bundle": "spago bundle-app --main Main --to dist/app.js",
    "serve": "spago bundle-app --main Main --to dist/app.js && npx http-server dist -p 8080 -c-1",
    "dev": "spago build --watch",
    "test": "spago test"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "affjax": "^12.0.0",
    "halogen": "^6.1.0",
    "halogen-hooks": "^0.5.0",
    "halogen-subscriptions": "^1.0.0",
    "purescript-argonaut": "^8.0.0",
    "purescript-argonaut-codecs": "^8.0.0",
    "purescript-argonaut-core": "^6.0.0",
    "purescript-argonaut-generic": "^6.0.0",
    "purescript-aff": "^7.0.0",
    "purescript-console": "^6.0.0",
    "purescript-effect": "^4.0.0",
    "purescript-prelude": "^6.0.0",
    "purescript-web-dom": "^3.0.0",
    "purescript-web-html": "^3.0.0"
  }
}
